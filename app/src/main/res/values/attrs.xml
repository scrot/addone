<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FloatingActionMenu">
        <attr name="fam_menu_state" format="enum">
            <enum name="collapse" value="2"/>
            <enum name="expanded" value="3"/>
        </attr>
        <attr name="fam_long_press" format="boolean" />
        <attr name="fam_menu_spacing" format="dimension" />
        <attr name="fam_size" format="dimension" />
        <attr name="fam_icon_src" format="integer" />
        <attr name="fam_icon_size" format="dimension" />
    </declare-styleable>
    <declare-styleable name="FloatingActionMenuButton">
        <attr name="famb_size" format="dimension" />
        <attr name="famb_icon_src" format="integer" />
        <attr name="famb_icon_size" format="dimension" />
        <attr name="famb_label" format="string" />
    </declare-styleable>
</resources>